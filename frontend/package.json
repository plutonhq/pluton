{
   "name": "@plutonhq/core-frontend",
   "description": "Pluton Core Frontend Library",
   "version": "0.0.1",
   "author": "Plutonhq",
   "license": "Apache-2.0",
   "publishConfig": {
      "access": "public"
   },
   "files": [
      "dist",
      "src",
      "README.md",
      "LICENSE"
   ],
   "sideEffects": [
      "**/*.css",
      "**/*.scss"
   ],
   "dependencies": {
      "@tanstack/react-query-devtools": "^5.90.2",
      "comlink": "^4.4.2",
      "dexie": "^4.2.1",
      "marked": "^16.2.1",
      "nanoid": "^5.0.9",
      "react-toastify": "^11.0.5",
      "react-tooltip": "^5.29.1",
      "react-window": "^1.8.11"
   },
   "peerDependencies": {
      "@tanstack/react-query": "^5.85.5",
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-router": "^7.9.5"
   },
   "devDependencies": {
      "@eslint/js": "^9.15.0",
      "@tanstack/eslint-plugin-query": "^5.62.1",
      "@types/node": "^22.10.2",
      "@types/react": "^18.3.12",
      "@types/react-dom": "^18.3.1",
      "@types/react-window": "^1.8.8",
      "@vitejs/plugin-react": "^4.3.4",
      "concurrently": "^9.2.1",
      "cross-env": "^10.0.0",
      "eslint": "^9.15.0",
      "eslint-config-prettier": "10.1.5",
      "eslint-plugin-import": "^2.31.0",
      "eslint-plugin-react-hooks": "^5.0.0",
      "eslint-plugin-react-refresh": "^0.4.14",
      "globals": "^15.12.0",
      "postcss": "^8.4.49",
      "prettier": "^3.4.2",
      "sass-embedded": "^1.82.0",
      "typescript": "^5.7.2",
      "typescript-eslint": "^8.15.0",
      "typescript-plugin-css-modules": "^5.1.0",
      "vite": "^6.0.3",
      "vite-plugin-comlink": "^5.3.0",
      "vite-plugin-pwa": "^1.1.0",
      "workbox-window": "^7.3.0"
   },
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.js",
         "default": "./dist/index.js"
      },
      "./components": {
         "types": "./dist/components/index.d.ts",
         "import": "./dist/components.js"
      },
      "./components/*": {
         "types": "./dist/components/*/index.d.ts",
         "import": "./dist/components/*/index.js"
      },
      "./routes": {
         "types": "./dist/routes/index.d.ts",
         "import": "./dist/routes.js"
      },
      "./routes/*": {
         "types": "./dist/routes/*/*.d.ts",
         "import": "./dist/routes/*/*.js"
      },
      "./routes/*/*": {
         "import": "./dist/routes/*/*"
      },
      "./components/*/*": {
         "import": "./dist/components/*/*"
      },
      "./services": {
         "types": "./dist/services/index.d.ts",
         "import": "./dist/services.js"
      },
      "./hooks": {
         "types": "./dist/hooks/index.d.ts",
         "import": "./dist/hooks.js"
      },
      "./utils": {
         "types": "./dist/utils/index.d.ts",
         "import": "./dist/utils.js"
      },
      "./context": {
         "types": "./dist/context/index.d.ts",
         "import": "./dist/context.js"
      },
      "./types": {
         "types": "./dist/@types/index.d.ts",
         "import": "./dist/@types/index.js"
      },
      "./@types/*": {
         "types": "./dist/@types/*.d.ts"
      },
      "./config/*": {
         "types": "./dist/config/*.d.ts",
         "import": "./dist/config/*.js"
      },
      "./styles/*": {
         "import": "./dist/styles/*"
      },
      "./router": {
         "types": "./dist/router.d.ts",
         "import": "./dist/router.js"
      },
      "./package.json": "./package.json"
   },
   "main": "./dist/index.js",
   "module": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "scripts": {
      "build": "tsc -b && vite build",
      "build:lib": "vite build --config vite.config.lib.ts && tsc -p tsconfig.lib.json",
      "build:lib:fast": "tsc -p tsconfig.lib.json",
      "build:lib:watch": "tsc -p tsconfig.lib.json --watch --preserveWatchOutput",
      "build:pkg": "npm run build:lib",
      "dev": "concurrently -n \"vite,tsc\" -c \"blue,green\" \"cross-env NODE_ENV=development vite\" \"npm run build:lib:watch\"",
      "lint": "eslint .",
      "preview": "vite preview",
      "prepublishOnly": "npm run build:lib"
   },
   "type": "module"
}
